{
  "name": "@elslander-cedric/lorgnette",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --proxy-config .angular-proxy-conf.json",
    "start-prod": "ng serve --prod --host=0.0.0.0 --proxy-config .angular-proxy-conf.json",
    "start-ssl": "ng serve --ssl true --disable-host-check --host=0.0.0.0 --proxy-config .angular-proxy-conf.json",
    "start-prod2": "ng serve --ssl true --ssl-key ssl/key.pem --ssl-cert ssl/certificate.pem --host=0.0.0.0 --disable-host-check --proxy-config .angular-proxy-conf.json --prod",
    "auto-deploy": "nodemon --watch 'src' -e ts --exec 'ng build && cp -r dist/* ../lorgnette-server/dist/'",
    "auto-deploy-prod": "nodemon --watch 'src' -e ts --exec 'ng build --prod && cp -r dist/* ../lorgnette-server/dist/'",
    "bundle": "gulp compress",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "test-live": "ng test",
    "test-report": "ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tag": "npm version patch && git push â€”tags origin master && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/platform-server": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@angular/service-worker": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.8",
    "@ngrx/store": "^4.1.1",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.2",
    "ts-node": "^4.0.2",
    "webpack": "^3.10.0",
    "xml2js": "^0.4.19",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0-beta.2",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/language-service": "^5.0.2",
    "@angular/service-worker": "^5.0.2",
    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.53",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "~4.0.1",
    "coveralls": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-tar": "^2.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.1"
  }
